.index-search {
	display: flex;
	align-items: center;
	height: 70rpx;
	margin: 70rpx 30rpx 30rpx 30rpx;
	background-color: #f7f7f7;border-radius: 50rpx;
}
.index-search text {
	font-size: 26rpx;
	margin-right: 26rpx;
	margin-left: 28rpx;
}
.index-search input {
	font-size: 28rpx;
	flex:1;
}
.index-list {
	margin: 0 30rpx;
}
.index-list-item {
	display: flex;
	margin-bottom: 34rpx;
}
.index-list-img {
	position: relative;
	width: 212rpx;
	height: 212rpx;
	border-radius: 30rpx;
	margin-right: 22rpx;
	overflow: hidden;
}
.index-list-img image {
	width: 100%;
	height: 100%;
}
.index-list-img text {
	position: absolute;
	bottom: 16rpx;
	left: 12rpx;
	color: white;
	font-size: 20rpx;
}
.index-list-text {
	font-size: 24rpx;
	line-height: 66rpx;
}

